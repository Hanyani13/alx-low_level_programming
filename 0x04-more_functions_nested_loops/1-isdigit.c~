#include <stdio>
#include "main.h"

/**
 * main - check the code
 *
 * Return: Always 0.
 */

int _isdigit(int c)
{
  c = '0';
  if (c == _isdigit)
    {
      return (1);
	}
  else
    {
      return (0);
    }
}
int main(void)
{
  int c;
  c = '0';
  while (c == _isdigit)
    {
    putchar('/n' c is a digit);
    }
  else{
    putchar('/n' c is not a digit);
  }
}
